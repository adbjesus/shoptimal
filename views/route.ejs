  <main id="main" class="main">

    <div class="pagetitle">
      <h1>Route</h1>
      <nav>
        <ol class="breadcrumb">
          <li class="breadcrumb-item"><a href="/Shopping">Shopping</a></li>
          <li class="breadcrumb-item active">Route</li>
        </ol>
      </nav>
    </div><!-- End Page Title -->

    <section class="section dashboard">
      <div class="row">
        <!-- Right side columns -->
        <div class="col-lg-4">

          <!-- Recent Activity -->
          <div class="card">
            <div class="card-body">
              <h5 class="card-title"><i class="bi bi-cart"></i> Cart</h5>

              <table class="table table-borderless datatable">
                <thead>
                  <tr>
                    <th scope="col">Product</th>
                    <th scope="col">Store</th>
                  </tr>
                </thead>
                <tbody id="cart-body">
                  <% cart.forEach(function(product, rowId) { %>
                    <tr id="cart-row-<%= rowId %>">
                      <th scope="row" style="vertical-align: middle;"><a href="#"><%= product.name %></a></th>
                      <th scope="row" style="vertical-align: middle;"><a href="#"><%= product.brand.name %></a></th>
                    </tr>
                  <% }) %>
                </tbody>
              </table>
            </div>
          </div><!-- End Recent Activity -->

        </div><!-- End Right side columns -->

      </div>
    </section>

  </main><!-- End #main -->

